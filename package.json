{
  "dependencies": {
    "vue": "^2.0.0-rc.5",
    "vue-resource": "^1.0.1",
    "vuex": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "browser-sync": "^2.15.0",
    "browserify": "^13.1.0",
    "jasmine": "^2.5.1",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "uglify-js": "^2.7.3",
    "vueify": "^9.2.4",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "npm run watch & npm run proxy & npm run serve",
    "test": "karma start karma.conf.js",
    "build": "browserify -t vueify -o build.js main.js",
    "minify": "uglifyjs -o build.min.js build.js",
    "watch": "watchify -t vueify -o build.js main.js",
    "proxy": "php -S localhost:3002 -t proxy",
    "serve": "browser-sync start -s -f 'index.html,build.js'"
  }
}
